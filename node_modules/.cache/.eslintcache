:"31"},{"filePath":"64","messages":"65","suppressedMessages":"66","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"67","messages":"68","suppressedMessages":"69","errorCount":0,"fatalErrorCount":0,"warningCount":2,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/index.js",[],[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/App.js",["70"],[],"import React from 'react';\n\n// import react router dom\nimport { BrowserRouter as Router, Route, Routes, BrowserRouter } from 'react-router-dom';\n\n// import pages\nimport Home from './pages/Home';\nimport ProductDetails from './pages/ProductDetails';\n\n// import components\nimport Header from './components/Header';\nimport Footer from './components/Footer';\nimport Sidebar from './components/Sidebar';\n\nconst App = () => {\n  return <div className='overflow-hidden'>\n    <Router>\n      <Header />\n      <Routes>\n        <Route path='/' element={<Home />} />\n        <Route path='/product/:id' element={<ProductDetails />} />\n      </Routes>\n      <Sidebar />\n      <Footer />\n    </Router>\n  </div>;\n};\n\nexport default App;\n","/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/contexts/SidebarContext.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/contexts/ProductContext.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/contexts/CartContext.js",["71"],[],"import React, {createContext, useState, useEffect} from 'react';\n\n// create context\nexport const CartContext = createContext();\n\nconst CartProvider = ({children}) => {\n  // cart state\n  const [cart, setCart] = useState([]);\n\n  // item amount state\n  const [itemAmount, setItemAmount] = useState(0);  \n\n  // total price state\n  const [total, setTotal] = useState(0);\n\n  useEffect(() => {\n    const price = '14.99';\n    const total = cart.reduce((accumulator, currentItem) => {\n      return accumulator + price * currentItem.amount;\n    },0);\n    setTotal(total);\n  });\n\n  // update item amount\n  useEffect(() => {\n    if(cart){\n      const amount = cart.reduce((accumulator, currentItem) => {\n        return accumulator + currentItem.amount;\n      }, 0);\n      setItemAmount(amount);\n    }\n  }, [cart]);\n\n  // add to cart\n  const addToCart = (product, rank) => {\n    const newItem = {...product, amount: 1};\n    // check if the item is already in the cart\n    const cartItem = cart.find((itme) =>{\n      return itme.rank === rank;\n    });\n    // if the item is already in the cart\n    if(cartItem){\n      const newCart = [...cart].map(item => {\n        if(item.rank === rank){\n          return {...item, amount: cartItem.amount + 1};\n        }else{\n          return item;\n        }\n      });\n      setCart(newCart);\n    }else{\n      setCart([...cart, newItem]);\n    }\n  };\n\n  // remove from cart\n  const removeFromCart = (rank) => {\n    const newCart = cart.filter((item) => {\n      return item.rank !== rank;\n    });\n    setCart(newCart);\n  };\n\n  //clear cart\n  const clearCart = () => {\n    setCart([]);\n  };\n\n\n  //increase amount\n  const increaseAmount = (rank)=> {\n    const cartItem = cart.find((item) => item.rank === rank);\n    addToCart(cartItem, rank);\n  };\n\n  //decrease amount\n  const decreaseAmount = (rank)=> {\n    const cartItem = cart.find((item) => {\n      return item.rank === rank;\n    });\n    if(cartItem){\n      const newCart = cart.map(item => {\n        if (item.rank === rank){\n          return {...item, amount: cartItem.amount - 1};\n        } else{\n          return item;\n        }\n      });\n      setCart(newCart);\n    }\n    if(cartItem.amount<2){\n      removeFromCart(rank);\n    }\n  };\n\n  return (<CartContext.Provider value={{cart, addToCart, removeFromCart, clearCart, increaseAmount, decreaseAmount, itemAmount, total}}>\n    {children}\n    </CartContext.Provider>);\n};\n\nexport default CartProvider;\n","/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/pages/Home.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/pages/ProductDetails.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/components/Header.js",["72"],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/components/Sidebar.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/components/Footer.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/components/CartItem.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/components/Product.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/components/Hero.js",["73","74"],[],{"ruleId":"75","severity":1,"message":"76","line":4,"column":50,"nodeType":"77","messageId":"78","endLine":4,"endColumn":63},{"ruleId":"79","severity":1,"message":"80","line":16,"column":3,"nodeType":"77","endLine":16,"endColumn":12,"suggestions":"81"},{"ruleId":"75","severity":1,"message":"82","line":2,"column":8,"nodeType":"77","messageId":"78","endLine":2,"endColumn":15},{"ruleId":"75","severity":1,"message":"83","line":4,"column":10,"nodeType":"77","messageId":"78","endLine":4,"endColumn":14},{"ruleId":"84","severity":1,"message":"85","line":23,"column":9,"nodeType":"86","endLine":23,"endColumn":25},"no-unused-vars","'BrowserRouter' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect contains a call to 'setTotal'. Without a list of dependencies, this can lead to an infinite chain of updates. To fix this, pass [cart] as a second argument to the useEffect Hook.",["87"],"'Sidebar' is defined but never used.","'Link' is defined but never used.","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement",{"desc":"88","fix":"89"},"Add dependencies array: [cart]",{"range":"90","text":"91"},[565,5