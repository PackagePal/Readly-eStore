item amount state\n  const [itemAmount, setItemAmount] = useState(0);  \n\n  // total price state\n  const [total, setTotal] = useState(0);\n\n  useEffect(() => {\n    const price = '14.99';\n    const total = cart.reduce((accumulator, currentItem) => {\n      return accumulator + price * currentItem.amount;\n    },0);\n    setTotal(total);\n  });\n\n  // update item amount\n  useEffect(() => {\n    if(cart){\n      const amount = cart.reduce((accumulator, currentItem) => {\n        return accumulator + currentItem.amount;\n      }, 0);\n      setItemAmount(amount);\n    }\n  }, [cart]);\n\n  // add to cart\n  const addToCart = (product, rank) => {\n    const newItem = {...product, amount: 1};\n    // check if the item is already in the cart\n    const cartItem = cart.find((itme) =>{\n      return itme.rank === rank;\n    });\n    // if the item is already in the cart\n    if(cartItem){\n      const newCart = [...cart].map(item => {\n        if(item.rank === rank){\n          return {...item, amount: cartItem.amount + 1};\n        }else{\n          return item;\n        }\n      });\n      setCart(newCart);\n    }else{\n      setCart([...cart, newItem]);\n    }\n  };\n\n  // remove from cart\n  const removeFromCart = (rank) => {\n    const newCart = cart.filter((item) => {\n      return item.rank !== rank;\n    });\n    setCart(newCart);\n  };\n\n  //clear cart\n  const clearCart = () => {\n    setCart([]);\n  };\n\n\n  //increase amount\n  const increaseAmount = (rank)=> {\n    const cartItem = cart.find((item) => item.rank === rank);\n    addToCart(cartItem, rank);\n  };\n\n  //decrease amount\n  const decreaseAmount = (rank)=> {\n    const cartItem = cart.find((item) => {\n      return item.rank === rank;\n    });\n    if(cartItem){\n      const newCart = cart.map(item => {\n        if (item.rank === rank){\n          return {...item, amount: cartItem.amount - 1};\n        } else{\n          return item;\n        }\n      });\n      setCart(newCart);\n    }\n    if(cartItem.amount<2){\n      removeFromCart(rank);\n    }\n  };\n\n  return (<CartContext.Provider value={{cart, addToCart, removeFromCart, clearCart, increaseAmount, decreaseAmount, itemAmount, total}}>\n    {children}\n    </CartContext.Provider>);\n};\n\nexport default CartProvider;\n","/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/pages/Home.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/pages/ProductDetails.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/components/Header.js",["86","87"],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/components/Sidebar.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/components/Footer.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/components/CartItem.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/components/Product.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/components/Hero.js",["88"],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/pages/Checkout.js",["89"],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/components/Dropdown.js",[],[],"/home/vania/Desktop/UA/2º semestre/TQS/Readly-eStore/src/pages/Tracking.js",["90"],[],{"ruleId":"91","severity":1,"message":"92","line":4,"column":50,"nodeType":"93","messageId":"94","endLine":4,"endColumn":63},{"ruleId":"95","severity":1,"message":"96","line":16,"column":3,"nodeType":"93","endLine":16,"endColumn":12,"suggestions":"97"},{"ruleId":"91","severity":1,"message":"98","line":2,"column":8,"nodeType":"93","messageId":"94","endLine":2,"endColumn":15},{"ruleId":"91","severity":1,"message":"99","line":12,"column":10,"nodeType":"93","messageId":"94","endLine":12,"endColumn":18},{"ruleId":"100","severity":1,"message":"101","line":30,"column":9,"nodeType":"102","endLine":30,"endColumn":25},{"ruleId":"91","severity":1,"message":"103","line":5,"column":9,"nodeType":"93","messageId":"94","endLine":5,"endColumn":23},{"ruleI